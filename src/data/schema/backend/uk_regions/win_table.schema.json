{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "List of wins for uk region win table",

	"type": "object",
	"required": [ "itt", "wins" ],
	"properties": {

		"itt": {
			"type": "object",
			"required": [ "name", "id" ],
			"properties": {
				"name": {
					"type": "string",
					"faker": "lorem.words"
				},
				"id": {
					"$ref": "$refs/uk-region.json#/id"
				}
			}
		},

		"wins": {
			"type": "object",
			"required": [ "hvc", "non_hvc" ],
			"properties": {
				"hvc": {
					"$ref": "$refs/win-table.json#/hvc-wins"
				},
				"non_hvc": {
					"$ref": "$refs/win-table.json#/non-hvc-wins"
				}
			}
		}
	}
}
