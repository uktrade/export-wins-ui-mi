{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "User info",

	"type": "object",
	"required": [ "name", "email", "last_login" ],
	"properties": {

		"name": {
			"type": "string",
			"faker": "name.findName"
		},

		"email": {
			"type": "string",
			"faker": "internet.email"
		},

		"last_login": {
			"type": "string",
			"faker": "date.past"
		}
	}
}
